<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mytest.mytestapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity
            android:name=".ShoppingMode"
            android:label="@string/title_activity_shopping_mode"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SideMENU"
            android:label="@string/title_activity_side_menu"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Settings"
            android:label="@string/title_activity_settings" />
        <activity android:name=".popAddItem"
            android:theme="@style/AppTheme.popupAdd"/>
    </application>

</manifest>